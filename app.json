{
  "expo": {
		"name": "Brite-Lite",
    "slug": "Brite-Lite",
    "version": "1.1.0",
    "description": "Create a glowing picture with brightly colored lights",
    "backgroundColor": "#000000",
    "orientation": "portrait",
    "androidStatusBar": {
      "hidden": true
    },
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eatthetoad.britelite"
    },
    "android": {
      "package": "com.eatthetoad.britelite",
      "versionCode": 4,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "blockedPermissions": [
        "android.permission.READ_MEDIA_AUDIO"
      ]
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": true
          },
          "android": {
            "newArchEnabled": true
          }
        }
      ],
			[
        "expo-navigation-bar",
        {
          "position": "absolute",
          "visibility": "hidden",
          "behavior": "inset-swipe",
          "backgroundColor": "#00000080"
        }
      ]
    ],
		"extra": {
      "expo-navigation-bar": {
        "position": "absolute",
        "visibility": "hidden",
        "behavior": "inset-swipe",
        "backgroundColor": "#00000080"
      }
		}
  }
}
