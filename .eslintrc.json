{
	"env": {
			"react-native/react-native": true,
			"es6": true
	},
	"extends": "plugin:react/recommended",
	"parserOptions": {
			"ecmaVersion": "latest",
			"sourceType": "module"
	},
	"plugins": ["react-native"],
	"rules": {
		"no-undef": "error",
		"no-unused-vars": "warn",
		"react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off"
	}
}
